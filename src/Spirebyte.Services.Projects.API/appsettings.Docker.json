{
  "app": {
    "name": "Spirebyte Projects Service",
    "version": "1"
  },
  "consul": {
    "enabled": true,
    "url": "http://spirebyte-consul:8500",
    "service": "projects-service",
    "address": "[hostname]",
    "port": "80"
  },
  "fabio": {
    "enabled": true,
    "url": "http://spirebyte-fabio:9999",
    "service": "projects-service"
  },
  "httpClient": {
    "type": "consul",
    "retries": 3,
    "services": {
      "identity": "identity-service"
    }
  },
  "logger": {
    "console": {
      "enabled": true
    },
    "elk": {
      "enabled": false,
      "url": "http://spirebyte-elk:9200"
    },
    "file": {
      "enabled": false,
      "path": "logs/logs.txt",
      "interval": "day"
    },
    "seq": {
      "enabled": true,
      "url": "http://spirebyte-seq:5341",
      "apiKey": "secret"
    }
  },
  "jaeger": {
    "enabled": true,
    "serviceName": "projects",
    "udpHost": "spirebyte-jaeger",
    "udpPort": 6831,
    "maxPacketSize": 0,
    "sampler": "const",
    "excludePaths": [
      "/",
      "/ping",
      "/metrics"
    ]
  },
  "mongo": {
    "connectionString": "mongodb://root:4UeQAJgTqbCwjPCH@spirebyte-mongo:27017/?authSource=admin",
    "database": "spirebyte-projects-service",
    "seed": true
  },
  "rabbitMq": {
    "hostnames": [
      "spirebyte-rabbitmq"
    ]
  },
  "redis": {
    "connectionString": "spirebyte-redis",
    "instance": "spirebyte-projects:"
  },
  "swagger": {
    "enabled": true,
    "reDocEnabled": false,
    "name": "v1",
    "title": "API",
    "version": "v1",
    "routePrefix": "docs",
    "includeSecurity": true
  },
  "metrics": {
    "tags": {
      "env": "production",
      "server": "vps"
    }
  },
  "vault": {
    "enabled": false,
    "url": "http://spirebyte-vault:8200",
    "kv": {
      "enabled": false
    },
    "pki": {
      "enabled": false
    },
    "lease": {
      "mongo": {
        "enabled": false
      }
    }
  }
}